{
  "expo": {
    "name": "Souleya",
    "slug": "souleya",
    "scheme": "souleya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C2A35"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.souleya.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2C2A35"
      },
      "package": "com.souleya.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
